{
  "name": "brower",
  "private": true,
  "version": "0.0.2",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"vite\" \"./pocketbase/pocketbase serve --http localhost:8090\"",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "start": "concurrently \"./pocketbase/bin/pocketbase serve --http :8090\" \"node server.cjs\"",
    "postinstall": "npm run build"
  },
  "dependencies": {
    "express": "^4.19.2",
    "pocketbase": "^0.21.5",
    "vue": "^3.4.37",
    "vue-i18n": "^9.14.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.1.2",
    "concurrently": "^7.4.0",
    "typescript": "^5.5.3",
    "vite": "^5.4.1",
    "vue-tsc": "^2.0.29"
  }
}
